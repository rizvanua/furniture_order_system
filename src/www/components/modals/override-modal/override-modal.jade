#quoteModal.modal.fade.bs-example-modal-sm(tabindex='-1', role='dialog', aria-hidden='true')
	.modal-dialog.modal-sm
		.modal-content
			.modal-header
				h4.modal-title Pricing Override
			.modal-body(style='text-align: center')
				h4 Enter Price Override
				form(name='newprojectform')
					.form-group(show-errors='')
						input.form-control(placeholder='Price', maxlength='100', data-ng-model='overridePrice')
			.modal-footer
				button.btn.btn-default(type='button', data-dismiss='modal')
					| Close
				button.btn.btn-primary.savemasterquote(type='button', data-ng-click='SaveOverride()')
					| Okay