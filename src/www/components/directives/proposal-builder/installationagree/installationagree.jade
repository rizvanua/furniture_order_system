- var initials=[{name:"Initials", value:""}, {name:"Initials", value:"Customer.date"}, {name:"Initials", value:""}]
	div.form-group.col-lg-10.col-xs-10
		label(class="col-xs-12")
			| Installation Agreement
			div(id="viewer",style="width:100%;height=100%;")
				object(data="{{data}}#view=FitH&toolbar=0&navpanes=0&scrollbar=0", type="application/pdf", style="width:100%;{{pdfheight}}")
	div.initial-group.col-lg-2.col-xs-2(style="margin-left:-5%;")
		- each initial in initials
			form-group.col-lg-12.col-xs-12(style="margin-top: 150px;")
				input.form-control
				label
					| #{initial.name}
div.wrap-signature
	.col-xs-6
		.signature
			canvas(ng-signature-pad="sign.instalagreecustomer", height="50", width="384")
			p
				| Customer Signature
		.signature
			canvas(ng-signature-pad="sign.instalagreecompany", height="50", width="384")
			p
				|Company Signature
	.col-xs-6
		.signature
			p 
				|{{Customer.date}}
			p
				|Date
		.signature
			p
				|{{Customer.date}}
			p
				|Date